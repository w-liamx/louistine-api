{
  "name": "louistine-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/app.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel src --out-dir build",
    "build": "npm run clean && npm run build-babel",
    "build-project": "npm run build",
    "start": "npm run build-project && node ./build/app.js"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "geoip-country": "^4.0.75",
    "glob": "^7.1.7",
    "http-errors": "~1.6.3",
    "jimp": "^0.16.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "nodemon": "^2.0.7"
  }
}
